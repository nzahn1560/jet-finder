{
    "name": "jetschoolusa-worker-api",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "wrangler dev",
        "deploy": "wrangler deploy",
        "migrate": "wrangler d1 migrations apply jetschoolusa-db",
        "migrate:local": "wrangler d1 migrations apply jetschoolusa-db --local",
        "migrate:create": "wrangler d1 migrations create",
        "db:backup": "wrangler d1 execute jetschoolusa-db --command \"SELECT sql FROM sqlite_master WHERE type='table'\" --output backup.sql"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20241127.0",
        "typescript": "^5.6.3",
        "wrangler": "^3.114.15"
    }
}
